  },
  "homepage": "https://github.com/YJK-7/logs-on-the-internet#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^2.3.0",
    "nodemon": "^2.0.20",
    "pg": "^8.8.0",
    "postgres": "^3.3.1"
  }
}
